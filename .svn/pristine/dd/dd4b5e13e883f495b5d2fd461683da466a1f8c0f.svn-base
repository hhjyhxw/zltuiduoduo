<!DOCTYPE html>
<html>
<head>
    <title>中奖记录表</title>
		<#include "/header.html">
</head>
<body>
<div id="icloudapp" v-cloak>
    <div v-show="showList">
        <div class="grid-btn">
			<#if shiro.hasPermission("zltdd:zltddawards:save")>
            <a class="btn btn-primary" @click="add"><i class="fa fa-plus"></i>&nbsp;新增</a>
			</#if>
			<#if shiro.hasPermission("zltdd:zltddawards:update")>
            <a class="btn btn-primary" @click="update"><i class="fa fa-pencil-square-o"></i>&nbsp;修改</a>
			</#if>
			<#if shiro.hasPermission("zltdd:zltddawards:delete")>
            <a class="btn btn-primary" @click="del"><i class="fa fa-trash-o"></i>&nbsp;删除</a>
			</#if>
        </div>
        <table id="jqGrid"></table>
        <div id="jqGridPager"></div>
    </div>

    <div v-show="!showList" class="panel panel-default">
        <div class="panel-heading">{{title}}</div>
        <form class="form-horizontal">
														                    <div class="form-group">
                        <div class="col-sm-2 control-label">用户id</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.userId" placeholder="用户id"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">被发展的用户id</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.invidatedUserId" placeholder="被发展的用户id"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">奖品配置id</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.prizeId" placeholder="奖品配置id"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">领取状态（0未领取 1已领取、2已过期）</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.status" placeholder="领取状态（0未领取 1已领取、2已过期）"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">创建时间</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.createTime" placeholder="创建时间"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">领取时间</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.receiveTime" placeholder="领取时间"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">奖品名称</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.prizeName" placeholder="奖品名称"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">领取过期时间</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.expireTime" placeholder="领取过期时间"/>
                        </div>
                    </div>
											                    <div class="form-group">
                        <div class="col-sm-2 control-label">修改时间</div>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" v-model="zltddAwards.modifyTime" placeholder="修改时间"/>
                        </div>
                    </div>
							            <div class="form-group">
                <div class="col-sm-2 control-label"></div>
                <input type="button" class="btn btn-primary" id="btnSaveOrUpdate" data-loading-text="保存中..." @click="saveOrUpdate" value="确定"/>
                &nbsp;&nbsp;<input type="button" class="btn btn-warning" @click="reload" value="返回"/>
            </div>
        </form>
    </div>
</div>

<script src="${request.contextPath}/statics/js/modules/zltdd/zltddawards.js?_${.now?long}"></script>
</body>
</html>