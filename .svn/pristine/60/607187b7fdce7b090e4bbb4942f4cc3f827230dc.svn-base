package com.icloud.web.common;

import org.springframework.stereotype.Controller;
import org.springframework.ui.ModelMap;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.ResponseBody;

import com.icloud.common.util.wx.WxConst;
import com.icloud.common.util.wx.model.JsSDK;
import com.icloud.model.subject.SubjectTopic;

/**
 * 类名称: JsSdkConfigController
 * 类描述: 
 * 创建人: zhangdehai
 * 创建时间:2018年8月15日 下午4:41:31
 */
@Controller
@RequestMapping(value = "/jsSdkConfig")
public class JsSdkConfigController {
	
	@RequestMapping("/getJsSdkConfig")
	@ResponseBody
	public JsSDK getJsSdkConfig(String url){
	
		try{
			return new JsSDK(WxConst.APPID,WxConst.APPID_SECRET,url);
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
	
	

}
